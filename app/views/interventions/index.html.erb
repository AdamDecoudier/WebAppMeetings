<p id="notice"><%= notice %></p>

<h1>Interventions</h1>

<table>
  <thead>
    <tr>
      <th>Id</th>
      <th>Titre</th>
      <th>Date</th>
      <th>Duree</th>
      <th>Description</th>
      <th>Nbgarcon</th>
      <th>Nbfille</th>
      <th>Nbelevesprevison</th>
      <th>Nbelevespresents</th>
      <th>Nbintavant</th>
      <th>Nbintapres</th>
      <th>Heuredebut</th>
      <th>Etablissement</th>
      <th>Parametrage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @interventions.each do |intervention| %>
      <tr>
        <td><%= intervention.id %></td>
        <td><%= intervention.titre %></td>
        <td><%= intervention.date %></td>
        <td><%= intervention.duree %></td>
        <td><%= intervention.description %></td>
        <td><%= intervention.nbGarcon %></td>
        <td><%= intervention.nbFille %></td>
        <td><%= intervention.nbElevesPrevison %></td>
        <td><%= intervention.nbElevesPresents %></td>
        <td><%= intervention.nbIntAvant %></td>
        <td><%= intervention.nbIntApres %></td>
        <td><%= intervention.heureDebut %></td>
        <td><%= intervention.etablissement.nom %></td>
        <td><%= intervention.parametrage.libelle %></td>
        <td><%= link_to 'Show', intervention %></td>
        <td><%= link_to 'Edit', edit_intervention_path(intervention) %></td>
        <td><%= link_to 'Destroy', intervention, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Intervention', new_intervention_path %>

